    "@nuxtjs/tailwindcss": "^6.6.6",
    "@pinia/nuxt": "^0.4.8",
    "@types/lodash": "^4.14.194",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "^3.4.1",
    "nuxt-simple-robots": "^2.1.2",
    "nuxt-simple-sitemap": "^2.4.11",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
