{
  "name": "beam-web",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "generate": "yarn lintfix && nuxi generate",
    "preview": "yarn lintfix && nuxi preview",
